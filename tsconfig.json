{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"module": "ESNext",
		"target": "esnext",
		"jsx": "react-native",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
